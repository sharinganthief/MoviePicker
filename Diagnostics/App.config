          <conversionPattern value="%property{MessageID}" />
        </layout>
      </parameter>
      <parameter>
        <parameterName value="@TraceStep " />
        <dbType value="Int32" />
        <size value="255" />
        <param name="Layout" type="log4net.Layout.RawPropertyLayout">
          <param name="Key" value="TraceStep" />
        </param>
      </parameter>
      <parameter>
        <parameterName value="@TraceStepKey " />
        <dbType value="String" />
        <size value="255" />
