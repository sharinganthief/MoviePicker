    <BulletDecorator Name="bulletDecorator" Background="#00FFFFFF">
      <BulletDecorator.Bullet>
        <Grid Width="16" Height="16">
          <Ellipse Name="Background" Height="14" Margin="1" Width="14" Fill="{TemplateBinding Control.Background}" Stroke="{TemplateBinding Control.BorderBrush}" StrokeThickness="{TemplateBinding Control.BorderThickness}" />
          <Ellipse Name="BackgroundOverlay" Height="14" Margin="1" Width="14" StrokeThickness="2" Stroke="{StaticResource HoverBrush}" Opacity="0" />
          <Ellipse Name="PressedEllipse" Height="14" Margin="1" Width="14" StrokeThickness="{TemplateBinding Control.BorderThickness}" Stroke="{StaticResource HoverBrush}" Opacity="0" />
          <Ellipse Name="CheckIcon" Height="6" Width="6" Opacity="0" Fill="{StaticResource GlyphBrush}" />
          <Ellipse Name="DisabledVisualElement" Height="14" Width="14" Opacity="0" Fill="{StaticResource DisabledBackgroundBrush}" />
